    "calculated_members": [
        {
            "name": "ScopeVariance",
            "dimension": "Measures",
            "formula": "IIf([Measures].[Planned] =0,0,\n[Measures].[Delivered] / [Measures].[Planned])",
            "format_string": "#0.00%"
        },
        {
            "name": "Planned",
            "dimension": "Measures",
            "formula": "CoalesceEmpty([Measures].[StoryPointsPlanned],0)",
            "format_string": "#,##0.00"
        },
        {
            "name": "Delivered",
            "dimension": "Measures",
            "formula": "CoalesceEmpty([Measures].[StoryPointsDelivered],0)",
            "format_string": "#,##0.00"
        }
    ]
