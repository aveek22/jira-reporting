            "calculated_members": [
                {
                    "name": "ScopeVariance",
                    "dimension": "Measures",
                    "formula": "IIf([Measures].[PlannedNoEmpty] =0,0,\n[Measures].[DeliveredNoEmpty] / [Measures].[PlannedNoEmpty])",
                    "format_string": "#0.00%"
                },
                {
                    "name": "PlannedNoEmpty",
                    "dimension": "Measures",
                    "formula": "CoalesceEmpty([Measures].[Planned],0)",
                    "format_string": "#,##0.00"
                },
                {
                    "name": "DeliveredNoEmpty",
                    "dimension": "Measures",
                    "formula": "CoalesceEmpty([Measures].[Delivered],0)",
                    "format_string": "#,##0.00"
                }
            ]
        }
